{
  "name": "shopping-cart-assignment",
  "version": "1.0.0",
  "description": "This is the base folder for the shopping cart exercise. You will have all the static assets and mock server responses available in this repo.",
  "main": "index.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "server-install": "npm install --prefix server",
    "install": "npm run client-install && npm run server-install",
    "server": "npm run watch --prefix server",
    "client": "npm start --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "npm run build --prefix client",
    "deploy": "npm run build && npm start --prefix server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ayanb12/shopping-cart-assignment.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ayanb12/shopping-cart-assignment/issues"
  },
  "homepage": "https://github.com/ayanb12/shopping-cart-assignment#readme",
  "devDependencies": {
    "concurrently": "^7.2.1"
  }
}
